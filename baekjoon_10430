import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		int A = s.nextInt();
		int B = s.nextInt();
		int C = s.nextInt();
		if (!(2 <= A && A <= 10000 && 2 <= B && B <= 10000 && 2 <= C && C <= 10000))
			return;
		
		System.out.println((A+B)%C);
		System.out.println((A%C+B%C)%C);
		System.out.println((A*B)%C);
		System.out.println((A%C)*(B%C)%C);
	}
}
