import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int N, money;
		
		N = s.nextInt();
		money = s.nextInt();
		
		int[] bal = new int[N];
		
		for (int i = 0; i < N; i++)
			bal[i] = s.nextInt();
		
		int cnt_bal = 0;
		
		for (int i = N - 1; i >= 0; i--) {
			cnt_bal += money / bal[i];
			money %= bal[i];
		}
		
		System.out.println(cnt_bal);
	}
}
