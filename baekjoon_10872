import java.util.Scanner;

public class Main {
	public static int facto(int n) {
		if (n == 1 || n == 0) return 1;
		return n * facto(n - 1);
	}
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		int N = s.nextInt();
		
		if (!(0 <= N && N <= 12))
			return;
		System.out.println(facto(N));
	}
}
